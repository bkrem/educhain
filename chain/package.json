{
  "name": "educhain-server",
  "version": "0.0.1",
  "scripts": {
    "start": "node app.js",
    "test": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha -- -R spec",
    "stop": "eris services stop",
    "servicelog": "eris services logs idi -f",
    "chainlog": "eris chains logs simplechain -f",
    "compile": "eris pkgs do --compiler=https://compilers.eris.industries:10113 --chain simplechain --address",
    "build": "docker build -t idiservice .; eris services rm --force idi; eris services start idi; eris services logs idi -f",
    "lint": "eslint ./js"
  },
  "dependencies": {
    "async": "^2.0.0",
    "bcrypt": "^0.8.7",
    "binstring": "^0.2.1",
    "body-parser": "^1.15.2",
    "eris-contracts": "^0.13.3",
    "express": "^4.14.0",
    "log4js": "^0.6.37",
    "multer": "^1.2.0",
    "toml-js": "0.0.8"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "istanbul": "^0.4.4",
    "mocha": "^2.5.3"
  }
}
